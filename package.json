{
  "name": "@mobilabs/spine",
  "version": "0.9.0",
  "description": "A tiny MVC framework inspired from Backbone.js",
  "main": "_dist/lib/spine.js",
  "minified": "_dist/lib/spine.min.js",
  "unpkg": "_dist/lib/spine.mjs",
  "module": "_dist/lib/spine.min.mjs",
  "bin": {},
  "scripts": {
    "build": "gulp build",
    "watch": "gulp watch",
    "dev": "gulp rundev",
    "apiserver": "node test/scripts/apiserver.js",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov mocha --require esm ./test/main.js",
    "display-coverage": "opener ./coverage/lcov-report/index.html",
    "check-coverage": "nyc check-coverage --statements 70 --branches 60 --functions 80 --lines 70",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "report": "nyc report",
    "makedist": "gulp makedist",
    "app": "gulp runapp",
    "prepare": "husky install",
    "doc": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/spine.git"
  },
  "keywords": [
    "ES6"
  ],
  "author": {
    "name": "Mobilabs",
    "email": "contact@mobilabs.fr",
    "url": "http://www.mobilabs.fr"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/spine/issues"
  },
  "homepage": "https://github.com/jclo/spine",
  "dependencies": {
    "@mobilabs/messenger": "^1.0.1",
    "@mobilabs/overslash": "^1.0.5"
  },
  "devDependencies": {
    "@mobilabs/es6kadoo": "^1.0.8",
    "body-parser": "^1.19.0",
    "chai": "^4.3.4",
    "cookie-parser": "^1.4.5",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "del": "^6.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
    "gulp-header": "^2.0.9",
    "gulp-replace": "^1.1.3",
    "gulp-uglify-es": "^3.0.0",
    "husky": "^7.0.4",
    "jsdom": "^18.0.1",
    "kadoo": "^1.0.4",
    "mocha": "^8.4.0",
    "node-fetch": "^2.6.6",
    "nyc": "^15.1.0",
    "open": "^8.4.0",
    "opener": "^1.5.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false
}
