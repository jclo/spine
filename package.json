{
  "name": "@mobilabs/spine",
  "version": "1.0.0-beta.1",
  "description": "A tiny MVC framework inspired from Backbone.js",
  "main": "_dist/lib/spine.js",
  "minified": "_dist/lib/spine.min.js",
  "unpkg": "_dist/lib/spine.mjs",
  "module": "_dist/lib/spine.min.mjs",
  "bin": {},
  "scripts": {
    "general": " --- GENERAL ---",

    "build:js": " --- JAVASCRIPT --- ",
    "build:js:dev": "node tasks/build.js.dev.js $1",
    "build:js:prod": "node tasks/build.js.prod.js $1",

    "build:css": " --- CSS --- ",

    "build:generic": " --- BUILD (GENERIC) --- ",

    "build:development": " --- BUILD (DEVELOPMENT) --- ",
    "build:dev": "npm run build:js:dev",

    "build:production": " --- BUILD (PRODUCTION) --- ",
    "build:skeleton:prod": "node tasks/build.skeleton.prod.js $1",
    "prebuild:prod": "npm run build:dev && npm run build:skeleton:prod",
    "build:prod": "npm run build:js:prod",

    "testing": " --- TESTING --- ",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov mocha --require esm ./test/main.js",
    "display:coverage": "open -a safari ./coverage/lcov-report/index.html",
    "check:coverage": "nyc check-coverage --statements 70 --branches 60 --functions 80 --lines 70",
    "report:coverage": "nyc report --reporter=text-lcov | coveralls",
    "report": "nyc report",

    "serving": " --- SERVING --- ",
    "server:api": "node test/scripts/apiserver.js",
    "server:dev": "http-server --port 8080",
    "server:prod": "http-server --port 8090",

    "watch:js:css": " --- WATCHING --- ",
    "watch:js": "nodemon --watch './src/**/*.js' --exec npm run build:dev",
    "watch": "npm run watch:js",

    "deployment": " --- DEPLOYMENT --- ",
    "dep:prod": "rm -rf _dist-$npm_package_version && npm run build:prod && cp -r _dist _dist-$npm_package_version",
    "dep:private": "npm run build:prod && node tasks/dep.private.js && sh tasks/compress.sh ${npm_package_name} ${npm_package_version} @mobilabs",

    "others": " --- OTHERS ---",
    "prepare": "husky install",
    "doc": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/spine.git"
  },
  "keywords": [
    "ES6"
  ],
  "author": {
    "name": "Mobilabs",
    "email": "contact@mobilabs.fr",
    "url": "http://www.mobilabs.fr"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/spine/issues"
  },
  "homepage": "https://github.com/jclo/spine",
  "dependencies": {
    "@mobilabs/messenger": "^1.0.3",
    "@mobilabs/overslash": "^1.0.7"
  },
  "devDependencies": {
    "@mobilabs/es6kadoo": "1.1.1",
    "chai": "^4.3.7",
    "cookie-parser": "^1.4.6",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "kadoo": "^1.0.7",
    "mocha": "^8.4.0",
    "node-fetch": "^2.7.0",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "terser": "^5.17.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false
}
